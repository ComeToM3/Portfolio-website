{
  "name": "hordearii-backend",
  "version": "1.0.0",
  "description": "Backend API pour le portfolio professionnel de Johan Dominguez",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "type-check": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:seed": "ts-node prisma/seed.ts",
    "db:studio": "prisma studio"
  },
  "keywords": [
    "portfolio",
    "api",
    "express",
    "typescript",
    "hordearii"
  ],
  "author": "Johan Dominguez <johan_dominguez@hotmail.com>",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.0.0",
    "@types/express": "^4.17.17",
    "@types/node": "^20.0.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "morgan": "^1.10.0",
    "prisma": "^5.0.0",
    "typescript": "^5.0.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.13",
    "@types/morgan": "^1.9.10",
    "nodemon": "^3.0.0",
    "ts-node": "^10.9.0"
  }
